{
  "name": "sumup-js",
  "version": "1.0.0",
  "description": "A module providing the standard SumUp JS configs.",
  "main": "./dist/index.js",
  "module": "./src/index.js",
  "author": "Felix Jung <felix.jung@sumup.com>",
  "license": "NO LICENSE",
  "private": true,
  "bin": "./dist/cli.js",
  "scripts": {
    "cleanup": "rimraf ./dist",
    "build": "BABEL_ENV=build NODE_ENV=production rollup --config",
    "test:watch": "yarn test --watchAll",
    "test": "jest",
    "develop": "BABEL_ENV=develop NODE_ENV=develop rollup --config --watch",
    "start": "yarn cleanup && yarn develop"
  },
  "files": [
    "dist",
    "babel.js",
    "eslint.js",
    "prettier.js"
  ],
  "dependencies": {
    "ajv": "^6.5.0",
    "lodash": "^4.17.10",
    "webpack-merge": "^4.1.2",
    "yargs": "^11.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.3",
    "@babel/core": "7.0.0-beta.49",
    "@babel/plugin-proposal-class-properties": "7.0.0-beta.49",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0-beta.49",
    "@babel/preset-env": "7.0.0-beta.49",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jest": "^21.17.0",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.8.2",
    "jest": "^23.1.0",
    "microbundle": "^0.4.4",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.13.3",
    "rimraf": "^2.6.2",
    "rollup": "^0.59.4",
    "rollup-plugin-babel": "^4.0.0-beta.4",
    "rollup-plugin-node-resolve": "^3.3.0"
  }
}
