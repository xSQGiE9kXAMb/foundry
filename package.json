{
  "name": "@sumup/sumup-js",
  "version": "0.0.1",
  "description": "A toolkit for JavaScript applications at SumUp.",
  "main": "index.js",
  "author": "Felix Jung <felix.jung@sumup.com>",
  "license": "MIT",
  "private": false,
  "bin": "cli.js",
  "scripts": {
    "cleanup": "rimraf ./dist",
    "build": "BABEL_ENV=build NODE_ENV=production rollup --config",
    "test:watch": "yarn test --watchAll",
    "test": "jest",
    "postinstall": "cp dist/*.js . && rm -rf dist",
    "develop": "BABEL_ENV=develop NODE_ENV=develop rollup --config --watch",
    "start": "yarn cleanup && yarn develop"
  },
  "files": ["dist"],
  "dependencies": {
    "ajv": "^6.5.0",
    "lodash": "^4.17.10",
    "plop": "^2.0.0",
    "webpack-merge": "^4.1.2",
    "yargs": "^11.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.0.0-beta.49",
    "@babel/plugin-proposal-class-properties": "7.0.0-beta.49",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0-beta.49",
    "@babel/preset-env": "7.0.0-beta.49",
    "babel-eslint": "^8.2.3",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jest": "^21.17.0",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.8.2",
    "jest": "^23.1.0",
    "microbundle": "^0.4.4",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.13.3",
    "rimraf": "^2.6.2",
    "rollup": "^0.59.4",
    "rollup-plugin-babel": "^4.0.0-beta.4",
    "rollup-plugin-copy": "^0.2.3",
    "rollup-plugin-node-resolve": "^3.3.0"
  }
}
