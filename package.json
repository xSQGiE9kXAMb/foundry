{
  "name": "sumup-js",
  "version": "1.0.0",
  "description": "A module providing the standard SumUp JS configs.",
  "main": "./dist/index.js",
  "module": "./src/index.js",
  "author": "Felix Jung <felix.jung@sumup.com>",
  "license": "NO LICENSE",
  "private": true,
  "bin": "./dist/cli.js",
  "scripts": {
    "cleanup": "rimraf ./dist",
    "build": "microbundle --format cjs --strict --compress false ./src/index.js ./src/cli.js",
    "test:watch": "yarn test --watchAll",
    "test": "jest",
    "develop": "microbundle watch --format cjs --strict --compress false ./src/index.js ./src/cli.js",
    "start": "yarn cleanup && yarn develop"
  },
  "files": [
    "dist",
    "babel.js",
    "eslint.js",
    "prettier.js"
  ],
  "dependencies": {
    "ajv": "^6.2.0",
    "babel-eslint": "^8.2.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.18.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jest": "^21.12.2",
    "eslint-plugin-prettier": "^2.6.0",
    "lodash": "^4.17.5",
    "microbundle": "^0.4.4",
    "prettier": "^1.11.1",
    "rimraf": "^2.6.2",
    "webpack-merge": "^4.1.2",
    "yargs": "^11.0.0"
  },
  "devDependencies": {
    "jest": "^22.4.2",
    "npm-run-all": "^4.1.2"
  }
}
